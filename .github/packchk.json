{
  "problemMatcher": [
    {
      "owner": "packchk",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^\\*\\*\\* (INFO|WARNING) M(\\d+): (.*) \\(Line (\\d+)\\)$",
          "file": 3,
          "line": 4,
          "code": 2
        },
        {
          "regexp": "^  (.*)$",
          "message": 1
        }
      ]
    }
  ]
}
